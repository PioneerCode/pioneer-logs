{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PioneerLogsConfiguration": {
    "ApplicationName": "Pioneer Logs",
    "ApplicationLayer": "Samples.AspNetCore.31",
    "MapToEcs": true,
    "Errors": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Usage": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Performance": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Diagnostics": {
      "WriteToFile": true,
      "WriteToConsole": true
    }
  }
}
