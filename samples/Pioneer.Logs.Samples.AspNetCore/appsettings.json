{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PioneerLogsConfiguration": {
    "ApplicationName": "Pioneer Logs",
    "ApplicationLayer": "Samples.AspNetCore",
    "Errors": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Usage": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Performance": {
      "WriteToFile": true,
      "WriteToConsole": true
    },
    "Diagnostics": {
      "WriteToFile": true,
      "WriteToConsole": true
    }
  }
}
